  <div class="table-title"><%= @request.trap.name %></div>
  <table class="trap-list-table">
    <thead>
      <tr>
        <td>Parameter</td>
        <td>Value</td>
      </tr>
    </thead>
    <tbody>
    <% @request.attributes.each do |value| %>
      <tr>
        <td><%= value[0].to_s.capitalize.gsub("_", " ") %></td>
        <td>
          <% if value[1].is_a?(Hash) %>
            <% value[1].each do |k,v| %>
              <table>
                <tr>
                  <td><%= k %></td>
                  <td><%= v %></td>
                </tr>
              </table>
            <% end %>
          <% else %>
            <td><%= value[1] %></td>
          <% end %>
        </td>
      </tr>
    <% end %>
    </tbody>
  </table>
  <div class="listing-manipulation">
    <div>
      <%= link_to 'Delete', request_path, method: :delete %>
    </div>
    <div>
      <%= link_to 'Back', trap_path %>
    </div>
  </div>