<p class="">Trap list</p>
<table class="trap-list">
  <thead>
    <tr>
      <td>#</td>
      <td>Trap name</td>
      <td>Request date</td>
    </tr>
  </thead>
  <tbody>
    <% @requests.each_with_index do |request, index| %>
      <tr>
        <td><%= index+1 %></td>
        <td><%= link_to request.dom_name, request %></td>
        <td><%= request.request_date.strftime("%F %T") %></td>
      </tr>
  <% end %>
  </tbody>
</table>